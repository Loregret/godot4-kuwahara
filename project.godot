; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Kuwahara Plus"
config/description="Small application that lets you to apply Kuwahara Effect on images. Also contains additional effects to play with."
run/main_scene="res://scripts/main_window.tscn"
config/features=PackedStringArray("4.0")
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/image="res://assets/logo.png"
boot_splash/fullsize=false
config/icon="res://assets/logo.png"

[display]

window/size/width=1920
window/size/height=1080

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

renderer/rendering_method="mobile"
vram_compression/import_etc=true
vram_compression/import_etc2=false
